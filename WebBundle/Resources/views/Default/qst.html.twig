                                <option value="2047">韩国</option>
                                <option value="2227">新西兰</option>
                                <option value="2228">爱尔兰</option>
                                <option value="2229">中国香港</option>
                                <option value="2230">新加坡</option>
                                <option value="1287">加拿大</option>            
                            </select>
                            <input id="tel" name="tel"  type="tel" class="QuickContact_Tel" placeholder="请填写手机号码">
                        </div>
                        <font class="inner-error"></font>
                        <input class="QuickContact_Submit" type="button" value="提交">
                        <div class="inner-footer">
                            芥末网资深留学顾问1对1为您免费解答留学问题
                        </div>
                    </div>
                    <!--输入电话 end-->
                    <!--输入微信 start-->
                    <div class="inner-tabarea hidden" data-right-area='2'>
                        <em>×</em>
                        <div class="success-area">
                            您已提交成功！
                        </div>
                        <div class="weixin-area input-txt-area">
                            <select id="wantt" class="QuickContact_Country">
                                <option value="">意向国家</option>
                                <option value="1">英国</option>
                                <option value="2">澳大利亚</option>
                                <option value="4">日本</option>
                                <option value="239">美国</option>
                                <option value="2047">韩国</option>
                                <option value="2227">新西兰</option>
                                <option value="2228">爱尔兰</option>
                                <option value="2229">中国香港</option>
                                <option value="2230">新加坡</option>
                                <option value="1287">加拿大</option>            
                            </select>
                            <input id="wx" name="wx" type="text" class="QuickContact_Weixin" placeholder="请填写微信号">
                        </div>
                        <font class="inner-error"></font>
                        <input class="QuickContact_Submit" type="button" value="提交">
                        <div class="inner-footer">
                            芥末网资深留学顾问1对1为您免费解答留学问题
                        </div>
                        <div class="news-right-erweima">
                            <img src="/files/ask/images/wx-ma.jpg">
                            <p>微信扫码咨询留学大咖</p>
                        </div>
                    </div>
                    <!--输入微信 end-->
                </div>
                <div class="ggContent">
                  <a><img alt='社区问答' src='/files/ask/images/qa-right-img01.png' /></a>
                </div>
                <div class="ggContent">
                    <a><img src="/files/ask/images/qa-right-img02.png" alt="问答页 GPA绩点计算器" /></a>
                </div>
                <div class="ggContent">
                    <a><img src="/files/ask/images/qa-right-img03.png" alt="问答页 美力克日本语学校" /></a>
                </div>
                <div class="ggContent">
                    <a><img src="/files/ask/images/qa-right-img04.jpg" alt="日本语言学校招生手册专题页" /></a>
                </div>
                <div class="ggContent">
                    <a><img src="/files/ask/images/qa-right-img05.jpg" alt="日本语言学校招生官视频专题页" /></a>
                </div>
                <!--热门板块开始-->
                <div class="qusRightList" style="display: none">
                  <h3>热门板块</h3>
                    <div class="clearfix margtop20"></div>
                </div>
                <!--相关问题列表结束-->
                </div>           
            </div>
            <!--右侧部分结束-->
        </div>
    </div>
</div>

<script type="text/javascript" src="/files/ask/js/jquery.min.js"></script>
<script type="text/javascript" src="/files/ask/js/bootstrap.min.js"></script>
