<script type="text/javascript" src="http://static.jiemo.net/proc/School/Public/Js/footer_bar_0b49cf15.js"></script><script type="text/javascript" src="http://static.jiemo.net/proc/School/Public/Js/jquery.cookie_31e1bcc4.js"></script><script type="text/javascript" src="http://static.jiemo.net/proc/School/Public/Js/college-info_61df4fc4.js"></script><script type="text/javascript" src="http://static.jiemo.net/Common/js/jquery.form_eca8a197.js"></script><script type="text/javascript" src="http://static.jiemo.net/Common/js/jquery-simulation_7ac25433.js"></script><script type="text/javascript" src="http://static.jiemo.net/Common/js/jquery-Validation_34fdd1b8.js"></script><script type="text/javascript" src="http://static.jiemo.net/Common/js/jquery-Alert_c16520e2.js"></script><script type="text/javascript" src="http://static.jiemo.net/Common/js/loginNew_a56e97ea.js"></script><script type="text/javascript" src="http://static.jiemo.net/proc/School/Public/Js/bounced_ed41076c.js"></script><!--twt-end/20151103(开启留学之旅)	-->


</script>


<div style="display:none;">

</div>


<div id="college-compare"></div>


<script>
	function autoScroll(obj){  
        $(obj).find("ul").animate({ marginTop : "-56px"},500,function(){  
            $(this).css({marginTop : "0px"}).find("li:first").appendTo(this);  
        })  
    }  
    $(function(){  
        setInterval('autoScroll(".apple")',2000);
    }) 
</script>

<script type="text/javascript" src="http://static.jiemo.net/proc/School/rankings/js/swiper.min_0e6a1846.js"></script><script type="text/javascript" src="http://static.jiemo.net/proc/School/rankings/js/all_5ff37158.js"></script><script type="text/javascript" src="http://static.jiemo.net/CDN/echarts/3.4.0/echarts.min_54c88fc6.js"></script><script type="text/javascript" src="http://static.jiemo.net/proc/School/rankings/js/echarts-all_87010402.js"></script>
<script>
    function changePage(page){
        window.location.href = 'http://www.eggelite.com/rank/{{ id }}/'+page+'';
    }
    var swiper = new Swiper('.swiper-container', {
        pagination: '.swiper-pagination',
        paginationClickable: true,
        autoplay:3000,
        loop:true,
        autoplayDisableOnInteraction:false,
        lazyLoadingInPrevNextAmount:1,
    });
</script>
